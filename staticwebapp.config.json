{
    "navigationFallback": {
      "rewrite": "/index.html"
    },
    "globalHeaders": {
      "content-type": {
        "*.js": "text/javascript",
        "*.mjs": "text/javascript",
        "*.jsx": "text/javascript",
        "*.tsx": "text/javascript"
      }
    },
    "routes": [
      {
        "route": "/*",
        "serve": "/index.html",
        "statusCode": 200
      }
    ]
  }